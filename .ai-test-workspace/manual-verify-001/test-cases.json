    {
      "case_id": "REQ-001-TC-001",
      "requirement_id": "REQ-001",
      "route": "/autoscan",
      "title": "页面加载后默认显示基线检测Tab及四个Tab选项",
      "precondition": "已登录系统",
      "steps": [
        { "step": 1, "action": "navigate", "target": "/autoscan", "description": "导航到任务自动下发配置页面" },
        { "step": 2, "action": "wait", "target": "getByRole('tab', { name: '基线检测' })", "description": "等待Tab栏加载完成" }
      ],
      "assertions": [
        { "assertion_id": "REQ-001-TC-001-A-001", "type": "element_visible", "description": "基线检测Tab应可见", "expected": "Tab可见", "selector": "getByRole('tab', { name: '基线检测' })" },
        { "assertion_id": "REQ-001-TC-001-A-002", "type": "element_visible", "description": "弱口令检测Tab应可见", "expected": "Tab可见", "selector": "getByRole('tab', { name: '弱口令检测' })" },
        { "assertion_id": "REQ-001-TC-001-A-003", "type": "element_visible", "description": "系统漏洞Tab应可见", "expected": "Tab可见", "selector": "getByRole('tab', { name: '系统漏洞' })" },
        { "assertion_id": "REQ-001-TC-001-A-004", "type": "element_visible", "description": "外部漏洞Tab应可见", "expected": "Tab可见", "selector": "getByRole('tab', { name: '外部漏洞' })" }
      ],
      "tags": ["tab", "default_load"]
    },

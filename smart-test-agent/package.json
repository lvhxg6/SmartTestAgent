{
  "name": "smart-test-agent",
  "version": "0.1.0",
  "private": true,
  "description": "PRD-Based UI Testing Agent System - AI-powered automated testing from PRD to Markdown reports",
  "keywords": [
    "testing",
    "automation",
    "playwright",
    "ai",
    "prd"
  ],
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:property": "turbo run test:property",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "turbo run typecheck",
    "db:generate": "turbo run db:generate --filter=@smart-test-agent/db",
    "db:push": "turbo run db:push --filter=@smart-test-agent/db",
    "db:migrate": "turbo run db:migrate --filter=@smart-test-agent/db"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
